<%= form_for @casestudy do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :question %><br />
    <%= f.text_field :question %>
  </p>
  <p>
    <%= f.label :headline %><br />
    <%= f.text_field :headline %>
  </p>
  <p>
    <%= f.label :strapline %><br />
    <%= f.text_field :strapline %>
  </p>
  <p>
    <%= f.label :story %><br />
    <%= f.text_area :story %>
  </p>
  <p><%= f.submit %></p>
<% end %>
