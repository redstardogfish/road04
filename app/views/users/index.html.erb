<% if !@users.blank? %>
  <ul>
    <% for user in @users %>
      <li><%= "#{user.email} | #{user.admin} | " %><%= link_to "Edit", edit_user_path(user) %> | <%= link_to "Destroy", user, :confirm => 'Are you sure?', :method => :delete %></li>
    <% end %>
  </ul>
<% else %>
  
<% end %>
<%= link_to "New user", new_user_path %>


<br /><br /><br />
<% if !@people.blank? %>
  <% for person in @people %>
    <%= image_tag person.headshot.url(:thumb), :width => "80px" %>
  <% end %>
<% else %>
  
<% end %>
